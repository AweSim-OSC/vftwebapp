(function() {
  var elem_id, $elem, content;
  elem_id = "#structural_paraview_<%= @session.id %>"
  $elem = $(elem_id);
  content = "<%= j link_to 'Launch with AweSim Connect', @conn.render(:awesim).strip, class: 'btn btn-primary' %>";
  $elem.show();
  $elem.popover({placement: 'top', html: true, content: content}).popover('show');
})();
